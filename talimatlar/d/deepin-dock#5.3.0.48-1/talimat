[sha1]
        #1 		= SKIP

[derle]
betik = cd dde-dock
betik = cmake . -GNinja -DCMAKE_INSTALL_PREFIX=/usr -DCMAKE_INSTALL_LIBDIR=lib -DDOCK_TRAY_USE_NATIVE_POPUP=YES
betik = ninja

[pakur]
betik = cd dde-dock
betik = DESTDIR="$PKG" ninja install
